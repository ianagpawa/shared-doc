{
    "name": "shared-doc-client",
    "version": "0.1.0",
    "author": "ian.agpawa",
    "private": true,
    "dependencies": {
        "react": "^18.2.0",
        "react-avatar": "^5.0.3",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "react-simple-wysiwyg": "^2.1.4",
        "react-use-websocket": "^4.2.0",
        "reactstrap": "^9.1.5"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "docker:build": "docker-compose -f docker-compose.client.yml build",
        "docker:start": "docker-compose -f docker-compose.client.yml up",
        "docker:stop": "docker-compose -f docker-compose.client.yml stop",
        "docker:clean-up": "docker-compose -f docker-compose.client.yml down",
        "docker:stop:clean": "npm run docker:stop && npm run docker:clean-up"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
